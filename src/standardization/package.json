{
  "name": "@telemetryflow/standardization",
  "version": "1.1.1",
  "description": "Module standardization tools for TelemetryFlow Core - Documentation generation, test coverage analysis, and quality gate validation",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:memory": "jest --detectOpenHandles --forceExit",
    "validate": "npm run lint && npm run test",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "standardization",
    "quality-gates",
    "ddd",
    "validation",
    "automation",
    "documentation-generation",
    "test-coverage",
    "nestjs",
    "typescript"
  ],
  "author": "DevOpsCorner Indonesia",
  "license": "Apache ver-2.0",
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "typescript": "^5.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^22.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devops-corner/telemetryflow-core.git",
    "directory": "src/standardization"
  },
  "bugs": {
    "url": "https://github.com/devops-corner/telemetryflow-core/issues"
  },
  "homepage": "https://github.com/devops-corner/telemetryflow-core/tree/main/src/standardization#readme"
}